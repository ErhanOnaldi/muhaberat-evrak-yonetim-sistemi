                $(document).ready(function() {
            // Get user data from server-side ViewBag
            const fullName = '@Html.Raw(ViewBag.CurrentUserFullName)' || 'Kullanıcı';
            const departmentName = '@Html.Raw(ViewBag.CurrentDepartmentName)' || '';
            const roleName = '@Html.Raw(ViewBag.CurrentRoleName)' || '';
            const displayDepartment = departmentName + (roleName ? ` (${roleName})` : '');

            // UI elementlerini güncelle (sidebar)
            $("#userName").text(fullName);
            $("#userTitle").text(displayDepartment);
            
            // Header user menu güncelle
            $("#userMenuName").text(fullName);
            $("#userMenuDepartment").text(displayDepartment);
        });
        $(document).ready(function () {
            // Sidebar logout button
            $("#logoutButton").click(function () {
                // Simple form submission to logout
                window.location.href = "@Url.Action("Logout", "Account")";
            });
            
            // Header logout button
            $("#headerLogoutButton").click(function () {
                // Simple form submission to logout
                window.location.href = "@Url.Action("Logout", "Account")";
            });
        });
    </script>
